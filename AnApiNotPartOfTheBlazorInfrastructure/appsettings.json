{
    "AzureAd": {
        "Instance": "https://login.microsoftonline.com/",
        "Domain": "{DOMAIN}",
        "TenantId": "{TENANT ID}",
        "ClientId": "{SERVER API APP CLIENT ID}",
        "CallbackPath": "/signin-oidc"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*",
    "AllowedOrigins": "https://production.origin"
}
